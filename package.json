{
  "name": "@mpaupulaire4/svelte-router",
  "description": "A simple client side router written in svelte",
  "version": "2.0.0",
  "author": "Michel Paupulaire <mpaupulaire4@gmail.com>",
  "repository": "git@github.com:mpaupulaire4/svelte-router.git",
  "license": "MIT",
  "svelte": "src/index.js",
  "module": "dist/index.m.js",
  "main": "dist/index.js",
  "private": false,
  "scripts": {
    "build": "rollup -c",
    "test:build": "rollup --config rollup.test.js",
    "test:serve": "sirv cypress/site --single --port 4000",
    "test:start": "cypress run",
    "test:site": "run-s test:build test:serve",
    "test": "run-p --race test:site test:start",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "cypress": "^3.6.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.11.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.0",
    "rollup-plugin-terser": "^5.1.2",
    "semantic-release": "^15.13.30",
    "sirv-cli": "^0.4.5",
    "svelte": "^3.12.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [
    "svelte",
    "router",
    "client",
    "routing"
  ],
  "files": [
    "src",
    "dist",
    "rollup",
    "README.md"
  ],
  "dependencies": {
    "lodash.template": "^4.5.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
