{
  "name": "@mpaupulaire4/svelte-router",
  "version": "4.0.0",
  "license": "MIT",
  "svelte": "./lib/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/svelte-router.js",
  "main": "./dist/svelte-router.umd.cjs",
  "author": "Michel Paupulaire <mpaupulaire4@gmail.com>",
  "repository": "git@github.com:mpaupulaire4/svelte-router.git",
  "type": "module",
  "files": [
    "dist",
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./dist/svelte-router.js",
      "require": "./dist/svelte-router.umd.cjs"
    }
  },
  "scripts": {
    "ci": "yarn lint && yarn check && yarn test run",
    "format": "prettier --write \"./src/**/*.{js,svelte,html,ts}\" \"./lib/**/*.{js,svelte,ts}\"",
    "lint": "eslint './src/**/*.{js,ts,svelte}' './lib/**/*.{js,ts,svelte}'",
    "lint:fix": "yarn lint --fix",
    "prelint:fix": "yarn format",
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.1",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitest/coverage-c8": "^0.23.2",
    "eslint": "^8.22.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "semantic-release": "^19.0.5",
    "svelte": "^3.50.1",
    "svelte-check": "^2.8.1",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vitest": "^0.23.2"
  },
  "dependencies": {
    "@crikey/stores-base-queue": "^0.0.10",
    "@crikey/stores-strict": "^0.0.13",
    "regexparam": "^2.0.1"
  },
  "peerDependencies": {
    "svelte": "^3.50.1"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
